cat <<EOF
OUTPUT_FORMAT("${OUTPUT_FORMAT}")
ENTRY(start)

SECTIONS {
  .text ${RELOCATING+${TEXT_START_ADDR}} : 
    {
	*(.text);
	*(.text1);
	*(.text2);
	${RELOCATING+_etext = .};
    }
  .lit . :
   {
	*(.lit);
	${RELOCATING+_elit = .};
   }
  .data . :
    {
	*(.data);
	*(.data1);
	*(.data2);
	${RELOCATING+_edata = .};
	${CONSTRUCTING+CONSTRUCTORS}
    }

  .bss  . :
    { 
	*(COMMON) 	
	*(.bss)
	${RELOCATING+_end = .};
    } 
}
EOF
