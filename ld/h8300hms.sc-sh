cat <<EOF
OUTPUT_FORMAT(${OUTPUT_FORMAT})
OUTPUT_ARCH(${ARCH})

MEMORY {
	rom   : o = 0x0000, l = 0x7fe0 
	duart : o = 0x7fe0, l = 16
	ram   : o = 0x8000, l = 29k
	hmsram: o = 0xfb80, l = 512
	}

SECTIONS 				
{ 					
.text :
	{ 					
	  *(.text) 				
	  *(.strings)
	} ${RELOCATING+ > ram}
.data  :
	{
	*(.data)
	} ${RELOCATING+ > ram}
.bss  :
	{
	${RELOCATING+ _bss_start = .}
	*(.bss)
	*(COMMON)
	${RELOCATING+ _end = .}
	} ${RELOCATING+ >ram}
}
EOF
