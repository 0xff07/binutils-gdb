31.Dest,26.Source2,21.0b110011000,12.0,11./,4.Source1::::and.ff r
	do_and (_SD, rDest, ~rSource1, ~rSource2);
31.Dest,26.Source2,21.0b110011000,12.1,11./::::and.ff l
