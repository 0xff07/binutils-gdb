# FR30 simulator testsuite.

if [istarget fr30*-*-*] {
    # load support procs
    # load_lib cgen.exp

    foreach src [lsort [glob -nocomplain $srcdir/$subdir/*.cgs]] {
	# If we're only testing specific files and this isn't one of them,
	# skip it.
	if ![runtest_file_p $runtests $src] {
	    continue
	}

	run_sim_test $src
    }
}
